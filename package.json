    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "allure-commandline": "^2.9.0",
    "chai": "^4.2.0",
    "del": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-mocha": "^6.0.0",
